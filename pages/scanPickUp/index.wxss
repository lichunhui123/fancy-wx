.top_tip{
    position:fixed;
    top:0;
    width:100%;
    height:68rpx;
    background: rgba(242,146,47,0.70);
    text-align: center;
    line-height: 68rpx;
    font-size: 24rpx;
    color: #FFFFFF;
}
.content{
    padding:68rpx 0 100rpx 0;
}
.list_branches{
    height:88rpx;
    background: #F8F8F8;
    font-size: 32rpx;
    color: #333333;
    line-height: 88rpx;
    padding-left: 54rpx;
    position: relative;
}
.list_branches:after{
    content:"";
    display:block;
    width:4rpx;
    height:30rpx;
    background: #5ADAD0;
    position: absolute;
    left:30rpx;
    top:32rpx;
}
.list_par{
    margin-left: 30rpx;
}
.list{
    display:flex;
    align-items: center;
    border-bottom: 1rpx solid #EBEBEB;
    padding:10rpx 0 32rpx 0;
}
.no_border{
    border-bottom:none;
}
.select_icon{
    width:30rpx;
    height:30rpx;
    padding-right:30rpx;
}
.select_icon image{
    width:30rpx;
    height:30rpx;
}
.list_name{
    margin-top: 20rpx;
    font-size: 0;
}
.cargonum{
  font-size: 28rpx;
  color: #333333;
}
.cargonumn{
  font-size: 28rpx;
  color: #333333;
  padding-right: 50rpx;
}
.name{
    font-size: 28rpx;
    color: #333333;
    line-height: 28rpx;
}
.odd_number{
    margin-left: 20rpx;
    font-size: 28rpx;
    color: #999999;
    line-height: 28rpx;
}
.take_code{
    font-size: 32rpx;
    color: #F2922F;
    line-height: 32rpx;
    font-family: PingFangSC-Medium;
}
.no_list{
    text-align: center;
}
.no_list image{
    width:450rpx;
    height:450rpx;
    margin-top:82rpx;
    margin-bottom: 20rpx;
}
.no_text{
    font-size: 24rpx;
    color: #333333;
    line-height: 24rpx;
}
.no_phone{
    text-align: center;
}
.no_phone image{
    width:450rpx;
    height:450rpx;
    margin-top:82rpx;
    margin-bottom: 20rpx;
}
.no_button{
    width:180rpx;
    height:60rpx;
    border: 2rpx solid #F2922F;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #F2922F;
    text-align: center;
    line-height: 60rpx;
    margin:0 auto;
    margin-top: 40rpx;
}
.bottom{
    background: #FFFFFF;
    box-shadow: 0 -4px 6px 0 rgba(0,0,0,0.06);
    height:100rpx;
    width:100%;
    display:flex;
    position:fixed;
    bottom:0;
}
.bottom_btn{
    width:100%;
    height:100%;
    background: #DDDDDD;
    font-size: 32rpx;
    color: #FFFFFF;
    line-height: 100rpx;
    text-align: center;
}
.active{
    background: #F2922F;
}
.confirm_bg{
    position: absolute;
    top:0;
    left:0;
    width:100vh;
    height:100vh;
    background: rgba(0,0,0,0.80);
    z-index: 2;
    overflow: hidden;
}
.confirm_con{
    position:absolute;
    width:400rpx;
    height:510rpx;
    top:50%;
    left:50%;
    transform: translate(-50%,-50%);
    z-index: 3;
}
.confirm_con image{
    width:400rpx;
    height:510rpx;
}
.confirm_text{
    text-align: center;
    width:400rpx;
    position: absolute;
    top:232.8rpx;
    right:0;
}
.tip_text01{
    font-size: 32rpx;
    color: #333333;
    line-height: 32rpx;
}
.tip_text02{
    font-size: 42rpx;
    color: #FF3B30;
    line-height: 42rpx;
    margin-top: 20rpx;
}
.tip_text03{
    font-size: 24rpx;
    color: #999999;
    line-height: 24rpx;
    margin-top: 20rpx;
}
.sure_btn{
    border: 1rpx solid #F2922F;
    border-radius: 30rpx;
    width:160rpx;
    height:60rpx;
    font-size: 28rpx;
    color: #F2922F;
    text-align: center;
    line-height: 58rpx;
    margin:30rpx auto 0;
}
