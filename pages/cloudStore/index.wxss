html, page {
    width: 100%;
    height: 100%;
    font-family: PingFangSC-Regular;
    background: #F8F8F8;
}
.header{
    width: 100%;
    height: 80rpx;
    padding:0 30rpx;
    box-sizing: border-box;
    background: white;
    display:flex;
    justify-content: space-between;
    align-items: center;
}
.header_left{
    font-size: 28rpx;
    color: #333;
    font-weight: 600;
    max-width: 467rpx;
    height: 80rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.header_left view{
  max-width:381rpx;
}
.head_title{
    max-width: 606rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 28rpx;
    line-height: 28rpx;
}
.address_icon{
    width: 44rpx;
    height: 44rpx;
    margin-right: 20rpx;
}
.right_icon{
    width: 12rpx;
    height: 22rpx;
    margin-left: 10rpx;
}
.header_right{
  width:203rpx;
  height:62rpx;
  background: #EEEEEE;
  border-radius: 34rpx;
  display:flex;
  align-items: center;
}
.header_right image{
  width:40rpx;
  height:40rpx;
  margin-left: 20rpx;
  margin-right: 10rpx;
}
.header_right view{
  font-size: 28rpx;
  color: #999999;
}
.saleTime{
    height: 60rpx;
    background:rgba(242,146,47,0.70);
    font-size: 24rpx;
    text-align: center;
    line-height: 60rpx;
    color: white;
}
.cloudSetting{
  width:100%;
  padding: 25rpx 30rpx;
  min-height:128rpx;
  background: #F8F8F8;
  display:flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.cloudSetting_left{
  width:470rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #666666;
}
.cloudSetting_time{
  margin-bottom: 10rpx;
}
.cloudSetting_icon{
  text-align: center;
  margin-right: 20rpx;
}
.cloudSetting_icon1{
  text-align: center;
}
.cloudSetting_right{
  display: flex;
  height: 90rpx;
  line-height: 90rpx;
}
.cloudSetting_right image{
  width:50rpx;
  height:50rpx;
  margin-bottom: 10rpx;
}
.cloudSetting_right .collectIcon{
  width:40rpx;
  height:38.7rpx;
  margin-bottom: 15.3rpx;
  margin-top: 7rpx;
}
.cloudSetting_right view{
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666666;
}
.cloudCoupon{
  height:86rpx;
  padding:0 30rpx;
  display:flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
}
.cloudCoupon_list{
  display:flex;
  max-width:610rpx;
}
.cloudCoupon_item{
  height:46rpx;
  font-size: 24rpx;
  padding:0 10rpx;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-right: 20rpx;
  position: relative;
  box-sizing: border-box;
}
.cloudCoupon_no{
  background-image: linear-gradient(196deg, #FF9D37 0%, #FF4F00 100%);
  color: #FFFFFF;
}
.cloudCoupon_no::after{
  content: '';
  width: 8rpx;
  height: 8rpx;
  border-radius:50%;
  background: white;
  position: absolute;
  top: -4rpx;
  right: 39rpx;
}
.cloudCoupon_no::before{
  content: '';
  width: 8rpx;
  height: 8rpx;
  border-radius:50%;
  background: white;
  position: absolute;
  bottom: -4rpx;
  right: 39rpx;
}
.onSale_text{
  font-size: 24rpx;
  line-height: 46rpx;
  display: flex;
  align-items: center;
}
.onSale_text text:first-child{
  font-size: 18rpx;
  margin-right: 5rpx;
}
.onSale_text text:last-child{
  font-size: 34rpx;
  line-height: 34rpx;
  margin-right: 10rpx;
}
.coupon_line{
  position: absolute;
  width:1px;
  height:46rpx;
  top:0;
  right:40rpx;
  border-left:1px dotted #fff;
}
.no_btn{
  margin-left: 22rpx;
}
.cloudCoupon_has{
  color: #FF5403;
  border: 1px solid #FF5403;
}
.coupon_icon{
  width:8rpx;
  height:46rpx;
  position: absolute;
  top:-1px;
  right:58rpx;
}
.coupon_icon image{
  width:100%;
  height:100%;
}
.cloudCoupon_btn{
  display:flex;
  align-items: center;
  width:80rpx;
}
.cloudCoupon_btn view{
  font-size: 24rpx;
  color: #666666;
  margin-right: 10rpx;
}
.cloudCoupon_btn image{
  width:22rpx;
  height:12rpx;
}
.discount_text{
  font-size: 24rpx;
  display:flex;
  align-items: center;
}
.discount_text text{
  font-size: 34rpx;
}
.has_btn{
  font-size: 22rpx;
  margin-left: 18rpx;
}
.data_content{
    display:flex;
    min-height: 800rpx;
}
.data_left_list{
    width:172rpx;
    height:100%;
    overflow-y: auto;
}
.navBar{
    width:172rpx;
    height:100rpx;
    position: relative;
}
.navBarText{
  width:100%;
  height:100%;
  font-size: 28rpx;
  color: #666666;
  line-height: 100rpx;
  background:  #F8F8F8;
  text-align: center;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;/*显示几行*/
}
.data_left_list .active{
    color: #333333;
    background: #FFFFFF;
    font-weight: 600;
}
.sale{
  width: 76rpx;
  height: 28rpx;
  background:linear-gradient(to top right, #FC5F3A, #FE964D);
  border-radius:15rpx 0 15rpx 0;
  font-size: 20rpx;
  color: white;
  text-align: center;
  line-height: 28rpx;
  position: absolute;
  top: 5rpx;
  right: 0;
  z-index: 2;
}
.sale1{
  width:50rpx;
}
.sale2{
  width:60rpx;
}
.goods_num{
  position: absolute;
  right:10rpx;
  top:10rpx;
  width:28rpx;
  height:28rpx;
  background: #FF3B30;
  color:#ffffff;
  border-radius: 50%;
  font-size: 16rpx;
  line-height: 28rpx;
  text-align: center;
}
.data_right_list{
    width:578rpx;
    overflow: auto;
    background: #ffffff;
}
.data_goods_item{
    width:538rpx;
    height:270rpx;
    margin:0 20rpx;
    display:flex;
    position: relative;
    border-bottom: 1rpx solid #EBEBEB;
}
.data_goods_item:last-of-type{
  border-bottom: none;
}
.data_goods_item_left{
    width:200rpx;
    height:200rpx;
    margin:20rpx 10rpx 20rpx 0;
}
.data_goods_item_left image{
    width:200rpx;
    height:200rpx;
    border-radius: 10rpx;
}
.data_goods_item_right{
    width:338rpx;
    margin: 15rpx 0 20rpx;
    position: relative;
}
.goods_name{
    max-height:76rpx;
    font-size: 28rpx;
    color: #333333;
    line-height: 38rpx;
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;/*显示几行*/
    font-family: PingFangSC-Medium;
    font-weight: 600;
}
.goods_spec{
    font-size: 24rpx;
    color: #999999;
    line-height: 24rpx;
    margin-top: 5rpx;
}
.goods_icon{
  display:flex;
  align-items: center;
  flex-wrap: wrap;
}
.goods_icon text{
    display: inline-block;
    border: 1rpx solid #FF5403;
    border-radius: 4rpx;
    font-size: 20rpx;
    color: #FF5403;
    height:28rpx;
    padding:0 10rpx;
    margin-right: 20rpx;
    margin-top: 10rpx;
}
.goods_price1{
    position: absolute;
    bottom:10rpx;
    left:0;
}
.goods_price_discount{
    font-size: 32rpx;
    line-height: 32rpx;
    color: #FF5403;
    margin-bottom: 10rpx;
    font-family: PingFangSC-Medium;
    font-weight: 600;
}
.noMargin{
  margin-bottom: 0;
}
.goods_price_origin{
    font-size: 20rpx;
    line-height: 20rpx;
    margin-top: 8rpx;
    color: #999999;
    text-decoration:line-through;
}
.disnum{
    font-size: 20rpx;
}
.disign{
    margin-left: 10rpx;
    font-size: 28rpx;
}
.addbox{
    width: 196rpx;
    height: 102rpx;
    display:flex;
    align-items: center;
    justify-content: flex-end;
    position: absolute;
    right:-10rpx;
    bottom:-10rpx;
}
.addbox .liue{
    width: 62rpx;
    height:102rpx;
    box-sizing: border-box;
    padding:30rpx 10rpx;
}
.addbox image{
    width: 42rpx;
    height:42rpx;
}
.addbox input{
    width: 40rpx;
    height: 42rpx;
    text-align: center;
    font-size: 28rpx;
}
.specialbox{
    width:100%;
    height:100rpx;
    display: flex;
    justify-content: center;
    position: fixed;
    z-index: 990;
    bottom: 77rpx;
}
.special{
    width:690rpx;
    height:100rpx;
    background: #FEF3E8;
    text-align: center;
    line-height: 52rpx;
    font-size: 20rpx;
    color: #333;
}
.again{
    color: #FF3B30;
    padding: 04rpx;
}
.nologin{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    padding-top: 82rpx;
}
.nologin .goodnodata{
    width: 450rpx;
    height:450rpx;
    margin-bottom: 40rpx;
}
.notitle{
    font-size: 24rpx;
    line-height: 24rpx;
    color: #333333;
}
/* 分享 */
.share{
  width: 94rpx;
  height: 94rpx;
}
.sharebtn{
  position: fixed;
  top: 70%;
  right: 20rpx;
  background-color:transparent;
  width: 94rpx;
  height: 94rpx;
  padding: 0;
}