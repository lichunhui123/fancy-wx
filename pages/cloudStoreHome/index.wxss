  margin-right: 5rpx;
}
.onSale_text text:last-child{
  font-size: 30rpx;
  margin-right: 10rpx;
}
.coupon_line{
  position: absolute;
  width:1px;
  height:46rpx;
  top:0;
  right:40rpx;
  border-left:1px dotted #fff;
}
.no_btn{
  margin-left: 22rpx;
}
.cloudCoupon_has{
  color: #FF5403;
  border: 1px solid #FF5403;
}
.coupon_icon{
