/* 优惠券 */
.cloudCoupon{
  height:86rpx;
  padding:0 30rpx;
  display:flex;
  align-items: center;
  justify-content: space-between;
  background: #F8F8F8;
}
.cloudCoupon_list{
  display:flex;
  max-width: 610rpx;
  flex-wrap: wrap;
}
.cloudCoupon_item{
  height:46rpx;
  font-size: 24rpx;
  padding:0 10rpx;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-right: 20rpx;
  position: relative;
  box-sizing: border-box;
}
.cloudCoupon_no{
  background-image: linear-gradient(196deg, #FF9D37 0%, #FF4F00 100%);
  color: #FFFFFF;
}
.cloudCoupon_no::after{
  content: '';
  width: 8rpx;
  height: 8rpx;
  border-radius:50%;
  background: white;
  position: absolute;
  top: -4rpx;
  right: 39rpx;
}
.cloudCoupon_no::before{
  content: '';
  width: 8rpx;
  height: 8rpx;
  border-radius:50%;
  background: white;
  position: absolute;
  bottom: -4rpx;
  right: 39rpx;
}
.onSale_text{
  font-size: 24rpx;
  line-height: 46rpx;
