page{
    background: #F8F8F8;
}
.top_bg{
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:300rpx;
    z-index: -1;
}
.order_status{
    font-size: 50rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 50rpx;
    margin:50rpx 0;
}
.order_obligation{
    margin:28rpx 0;
    text-align: center;
    color: #FFFFFF;
}
.order_obligation view:first-of-type{
    font-size: 56rpx;
    line-height: 56rpx;
}
.order_obligation view:last-of-type{
    font-size: 24rpx;
    line-height: 24rpx;
    margin-top: 14rpx;
}
.water_user_info{
    width:710rpx;
    box-sizing: border-box;
    margin: 10rpx auto 0;
    padding:0 20rpx;
    background: #fff;
    border-radius: 10rpx;
    position:relative;
}
.water_info_top{
    height:80rpx;
    box-sizing: border-box;
    border-bottom:1rpx solid #EBEBEB;
    display:flex;
    align-items: center;
    justify-content:flex-start;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #333333;
}
.water_info_top image{
    width:34rpx;
    height:34rpx;
    margin-right: 13rpx;
}
.water_info_center{
    padding:10rpx 0;
}
.water_info_list{
    font-size: 28rpx;
    color: #333333;
    line-height: 48rpx;
}
.water_info_icon{
    position:absolute;
    right:0;
    bottom:0;
    width:86rpx;
    height:86rpx;
}
.water_info_icon image{
    width:100%;
    height:100%;
}
.order_text{
    color: #333333;
}
.order_info{
    margin: 10rpx auto 0;
    width:710rpx;
    background: #fff;
    border-radius: 10rpx;
}
.order_info_top{
    height:88rpx;
    box-sizing: border-box;
    border-bottom:1rpx solid #EBEBEB;
    display:flex;
    align-items: center;
    justify-content:space-between;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #999999;
    margin:0 20rpx;
}
.order_info_list{
    width:670rpx;
    margin:0 20rpx;
    padding:20rpx 0;
    border-bottom:1rpx solid #EBEBEB;
    display:flex;
}
.order_info_list:last-of-type{
    border:none;
}
.goods_img{
    width:160rpx;
    height:160rpx;
    position:relative;
}
.goods_pic{
    width:100%;
    height:100%;
}
.goods_icon{
    position:absolute;
    left:0;
    top:0;
    width:86rpx;
    height:86rpx;
}
.goods_con{
    margin-left: 20rpx;
    width:490rpx;
}
.goods_name{
    width:100%;
    margin-top: 10rpx;
    font-size: 28rpx;
    color: #333333;
    line-height: 28rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.goods_desc{
    margin-top: 20rpx;
    font-size: 24rpx;
    color: #999999;
    line-height: 24rpx;
    height:64rpx;
}
.goods_price{
    font-size: 28rpx;
    color: #333333;
    line-height: 28rpx;
}
.goods_price text{
    font-size: 24rpx;
    line-height: 24rpx;
    margin-right: 6rpx;
}
.order_price{
    width:710rpx;
    box-sizing: border-box;
    padding:30rpx 20rpx 26rpx 30rpx;
    background: #fff;
    border-radius: 10rpx;
    font-size:28rpx;
    line-height: 28rpx;
    color: #333333;
    margin:10rpx auto 0;
}
.order_price_text{
    display: flex;
    justify-content: space-between;
    margin-bottom: 22rpx;
}
.order_price_text view:last-of-type{
    font-size: 30rpx;
}
.order_total_price{
    text-align: right;
    font-size: 28rpx;
    color: #333333;
    line-height: 36rpx;
    padding-top:12rpx;
}
.order_total_price text{
    color:#FF3B30;
    font-size: 36rpx;
}
.order_total_price text:first-of-type{
    font-size: 28rpx;
    margin-right: 2rpx;
}
.order_code{
    box-sizing: border-box;
    width:710rpx;
    background: #fff;
    margin:10rpx auto 20rpx;
    border-radius: 10rpx;
    padding:30rpx 20rpx 10rpx;
}
.code_text{
    margin-bottom: 20rpx;
    font-size: 28rpx;
    color: #333333;
    line-height: 28rpx;
}
.code_text01{
    display: flex;
    align-items: center;
}
.code_copy{
    width:64rpx;
    height:33rpx;
    border: 1rpx solid #F2922F;
    border-radius: 10rpx;
    font-size: 24rpx;
    color: #F2922F;
    line-height: 33rpx;
    text-align: center;
    margin-left: 10rpx;
}
.footer{
    width: 750rpx;
    height: 100rpx;
    background: #FFFFFF;
    box-shadow: 0 -4rpx 6rpx 0 rgba(0,0,0,0.06);
    display: flex;
    align-items: center;
    justify-content: flex-end;
    position: fixed;
    bottom: 0;
    left: 0;
}
.iphone{
  position: fixed;
  bottom: 34rpx;
}
.tabbarIphone{
  width: 100%;
  height: 34rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
}
.order_obligation_detail{
    margin-bottom: 109rpx;
}
.footer view{
    width:188rpx;
    height:70rpx;
    text-align: center;
    line-height: 70rpx;
    font-size: 26rpx;
    border-radius: 34rpx;
    box-sizing: border-box;
    margin-right: 30rpx;
}
.footer view:first-of-type{
    border: 1rpx solid #DDDDDD;
    color:#333333;
}
.footer view:last-of-type{
    background: #F2922F;
    color:#ffffff;
}