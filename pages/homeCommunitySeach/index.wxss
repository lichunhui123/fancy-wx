/* pages/homeCommunitySeach/index.wxss */
html, page {
  width: 100%;
  height: 100%;
  font-family: PingFangSC-Regular;
}

.locationWrap {
  width: 100%;
}
.search{
  height: 86rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.seachinp{
  width: 690rpx;
  height: 66rpx;
  border-radius: 66rpx;
  background: #eee;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
  position: relative;
}
.inp{
  width: 400rpx;
}
.seachinp .delinp{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 90%;
  top:13rpx;
}
.seachinp image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}
.searchplace{
  color: #999; 
}
.community_item{
  min-height: 165rpx;
  box-sizing: border-box;
  margin-left: 30rpx;
  display: flex;
  border-bottom: 1rpx solid #EBEBEB;
}
.coitem_left{
  width: 100rpx;
  min-height: 165rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.coitem_left image{
  width: 100rpx;
  height: 100rpx;
}
.coitem_right{
  width: 620rpx;
  min-height: 165rpx;
  display: flex;
  box-sizing: border-box;
  padding:30rpx 30rpx 30rpx 20rpx;
  flex-direction: column;
  justify-content: space-between;
  position: relative;
}
.coitem_name{
  font-size: 28rpx;
  color: #333333;
  display: flex;
  justify-content: space-between;
}
.coitem_title{
  font-size: 24rpx;
  color: #999;
  width:100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.coitem_address{
  box-sizing: border-box;
  padding-right: 42rpx;
  font-size: 24rpx;
  color: #666;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;/*显示几行*/
}
.near_formerly{
  width: 100%;
  box-sizing: border-box;
  background:#fff;
}

.distance{
  font-size: 24rpx;
  color: #F2922F;
  text-align: right;
}
.locaright{
  width: 12rpx;
  height: 22rpx;
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
  z-index: 88;
}
.nullData {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  margin-top: 82rpx;
}
.nullimg {
  width: 450rpx;
  height: 450rpx;
  margin: 0 auto;
  display: block;
}

.nullTitle {
  font-size: 24rpx;
  color: #333333;
  text-align: center;
  margin-top: 20rpx;
}