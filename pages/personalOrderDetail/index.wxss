    text-align: right;
    font-size: 28rpx;
    color: #333333;
    line-height: 36rpx;
    padding-top:12rpx;
}
.order_total_price text{
    color:#FF3B30;
    font-size: 36rpx;
}
.order_total_price text:first-of-type{
    font-size: 28rpx;
    margin-right: 2rpx;
}
.order_code{
    box-sizing: border-box;
    width:710rpx;
    background: #fff;
    margin:10rpx auto 20rpx;
    border-radius: 10rpx;
    padding:30rpx 20rpx 10rpx;
}
