    line-height: 28rpx;
    color: #333333;
    margin:10rpx auto 0;
}
.order_price_text{
    display: flex;
    justify-content: space-between;
    margin-bottom: 22rpx;
}
.order_price_text view:last-of-type{
    font-size: 30rpx;
}
.order_price_text span{
    font-size: 24rpx;
