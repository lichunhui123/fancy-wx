  left: 0;
  background: white;
}
.order_obligation_detail{
    padding-bottom: 109rpx;
}
.footer view{
    width:50%;
    height:100rpx;
    text-align: center;
    line-height: 100rpx;
    font-size: 32rpx;
    box-sizing: border-box;
    color: #FFFFFF;
}
.footer view:first-of-type{
    background: #333333;
}
.footer view:last-of-type{
    background: #F2922F;
}
.footer_btn{
    background: #F2922F;
    width:100%;
    height:100rpx;
    font-size: 32rpx;
    color: #FFFFFF;
