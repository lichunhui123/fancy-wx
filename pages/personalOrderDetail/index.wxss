    margin-right: 6rpx;
}
.goods_price .original_price{
    font-size: 20rpx;
    color: #999999;
    line-height: 24rpx;
    margin-left: 20rpx;
    text-decoration: line-through;
}
.order_price{
    width:710rpx;
    box-sizing: border-box;
    padding:30rpx 20rpx 26rpx 30rpx;
    background: #fff;
    border-radius: 10rpx;
