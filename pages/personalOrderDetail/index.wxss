    line-height: 34rpx;
    text-align: center;
    margin-left: 10rpx;
}
.order_express{
  width:710rpx;
  padding:0 20rpx 20rpx;
  box-sizing: border-box;
  margin:20rpx auto 42rpx;
  background: #fff;
  font-size: 28rpx;
  color: #333333;
  border-radius: 10rpx;
}
.order_express view:nth-child(1){
  line-height: 80rpx;
  border-bottom: 1rpx solid #EBEBEB;
}
.order_express view:nth-child(2),.order_express view:nth-child(3){
  line-height: 28rpx;
  margin-top: 20rpx;
}
.footer{
    width: 750rpx;
    height: 100rpx;
    background: #FFFFFF;
    box-shadow: 0 -4rpx 6rpx 0 rgba(0,0,0,0.06);
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: fixed;
    bottom: 0;
    left: 0;
}
.iphone{
  position: fixed;
  bottom: 34rpx;
}
.tabbarIphone{
  width: 100%;
  height: 34rpx;
