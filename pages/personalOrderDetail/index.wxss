    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    position:relative;
}
.order_info_top{
    height:88rpx;
    box-sizing: border-box;
    border-bottom:1rpx solid #EBEBEB;
    display:flex;
    align-items: center;
    justify-content:space-between;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #999999;
    margin:0 20rpx;
}
.order_discount_icon image{
    width:184rpx;
    height:50rpx;
}
.order_info_icon{
