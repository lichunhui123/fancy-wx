  line-height: 28rpx;
}
.order_type{
  position: relative;
  width: 98rpx;
  height: 34rpx;
  margin-right: 10rpx;
}
.order_type image {
  position: absolute;
  left: 0;
  top: 0;
  width: 98rpx;
  height: 34rpx;
} 
.order_type text{
  width: 98rpx;
  height: 34rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  font-size: 20rpx;
  color: #FFFFFF;
  line-height: 34rpx;
  text-align: center;
}
.order_activity_icon{
    position:relative;
}
.order_activity_icon image{
    width:100rpx;
    height:28rpx;
    border-radius: 15rpx 0 15rpx 0;
}
.order_activity_icon span{
    position: absolute;
