  box-shadow: 0 2px 8px 0 rgba(0,0,0,0.06);
  border-radius: 8px;
}
.detailc1_img{
  width: 100%;
  height:114rpx;
  overflow: hidden;
  position: relative;
  word-spacing: 0;
}
.detailc1_img image{
  width: 100%;
  height: 114rpx;
}
.detailc1_img_title{
  width: 100%;
  height: 114rpx;
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
}
.price_left{
  width: 437rpx;
  height: 114rpx;
