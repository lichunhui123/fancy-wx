}
.detailc1_sales{
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid  #EBEBEB;
  font-size: 24rpx;
  color: #999999;
}
.share{
  width:188rpx;
  height:50rpx;
  border-radius: 25rpx 0 0 25rpx;
  overflow: hidden;
  position: absolute;
  top:19rpx;
  right: 0;
}
.share button{
  height:50rpx;
  line-height: 50rpx;
  border: none;
