}
.swiper-num{
  position:absolute;
  bottom:124rpx;
  left:50%;
  transform:translate(-50%,0);
  height:32rpx;
  padding:0 14rpx;
  opacity: 0.5;
  background: #000000;
  border-radius: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  line-height: 32rpx;
  color: #FFFFFF;
}
.detail_content{
