/* pages/groupGoodDetail/index.wxss */
html, page {
  width: 100%;
  height: 100%;
  font-family: PingFangSC-Regular;
}

.detailWrap {
  width: 100%;
}
.sale{
  position: absolute;
  top: 30rpx;
  left: 0;
  width: 184rpx;
  height: 50rpx;
  z-index: 9;
}
.swiper-content{
  width: 100%;
  height:750rpx;
  background:#fff;
  position:relative;
}
.swiper{
  height:750rpx;
}
.swiper-item{
