/* pages/location/index.wxss */

html, page {
  width: 100%;
  height: 100%;
  font-family: PingFangSC-Regular;
  background: #F8F8F8;
}
.locationWrap {
  width: 100%;
  position: relative;
}
.cloudStoreList{
  position: absolute;
  top: 80rpx;
}
.search{
  /* height: 96rpx; */
  display: flex;
  justify-content: center;
  padding-top: 10rpx;
  box-sizing: border-box;
  position: absolute;
  left: 30rpx;
  top: 3rpx;
}
.seachinp{
  width: 690rpx;
  height: 68rpx;
  line-height: 68rpx;
  border: 2rpx solid #FFD925;
  border-radius: 34rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #fff;
  background: rgba(255,92,0,0.30);
}
.seachinp image{
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}
.searchplace{
  color: rgba(255,255,255,0.60);
}
.header {
  width: 100%;
}
.header_top{
  width: 100%;
  height: 82rpx;
  display: flex;
  justify-content: center;
  align-items: center; 
}
.cloude_title{
  font-size: 32rpx;
  color: #fff;
  margin: 0 14rpx;
}
.header_top .collect_left,.header_top .collect_right{
  width: 44rpx;
  height: 2rpx;
}
.head_img {
  width: 100%;
  height: 315rpx;
  box-sizing: border-box;
}
.current_comm {
  width: 710rpx;
  /* height: 202rpx; */
  background: #fff;
  box-sizing: border-box;
  box-shadow: 0 2rpx 8rpx 0 rgba(0, 0, 0, 0.12); 
  border-radius: 16rpx;
  margin: 0 20rpx 30rpx;
}
.current_comm_count{
  display: flex;
  padding: 30rpx 20rpx;
  position: relative;
}
.before_bottom_left,.current_comm_left{
  width: 140rpx;
  height: 140rpx;
  margin-right: 20rpx;
  display: flex;
  align-items: center;
}
.current_comm_left image,.before_bottom_left image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}
.current_comm_branchId{
  font-size: 28rpx;
  line-height: 28rpx;
  height: 28rpx;
  color: #333;
  font-weight: 600;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.current_comm_type{
  display: flex;
  margin-top: 14rpx;
  align-items: center;
}
.current_comm_type1{
  height: 28rpx;
  border: 2rpx solid  #FF5403;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #FF5403;
  text-align: center;
  padding: 0 10rpx;
  margin: 14rpx 0;
  display: inline-block;
}
.current_comm_type view{
  height: 28rpx;
  border: 2rpx solid  #FF5403;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: #FF5403;
  text-align: center;
  padding: 0 10rpx;
  margin-bottom: 14rpx;
}
.current_comm_address{
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient:vertical;
  -webkit-line-clamp: 2;
}
/* 已收藏门店 */
.collect,.before_branch{
  background: #fff;
  margin: 20rpx 0 30rpx;
}
.collect_count_top{
  width: 100%;
  height: 82rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 600;
  color:#333
}
.collect_count_title{
  margin: 0 14rpx;
}
.collect_count_top image{
  width: 44rpx;
  height: 2rpx;
}
.collect_count,.before_count{
  width: 710rpx;
  margin: 30rpx 20rpx;
  position: relative;
  border-bottom: 1rpx solid #EBEBEB;
}
.collect_count{
  max-height: 535rpx;
  overflow-y: hidden;
}
.collect_count_bottom{
  position: relative;
}
.collect_count_bottom,.before_branch_bottom{
  display: flex;
  margin-bottom: 30rpx;
  margin-left: 20rpx;
}
.collect_count_bottom image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.cloudList_icon{
  width: 40rpx;
  height: 38.7rpx;
  position: absolute;
  top: 29rpx;
  right:160rpx;
}
.cloudList_icon1{
  width: 40rpx;
  height: 38.7rpx;
  position: absolute;
  top: 29rpx;
  right:20rpx;
}
.collect_lookmore{
  display: flex;
  justify-content: center;
  margin-top: 20rpx;
  padding-bottom: 30rpx;
}
.collect_lookmore{
  font-size: 24rpx;
  color: #666;
  height: 28rpx;
  line-height: 28rpx;
}
.lookbottomimg{
  width: 12rpx;
  height: 22rpx;
  margin-left: 11rpx;
  margin-top: 2rpx;
}
.collect_count_branchId,.before_bottom_branchId{
  font-size: 28rpx;
  line-height: 28rpx;
  height: 28rpx;
  width: 350rpx;
  color: #333;
  font-weight: 600;
  font-family: PingFangSC-Medium;
  display:-webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.openbtn{
  width: 130rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #DDDDDD;
  border-radius: 30rpx;
  text-align: center;
  margin-top: 14rpx;
}
.openbtn text{
  color: #fff;
  font-size: 28rpx;
  line-height: 28rpx;
}
/* 曾用门店 *//* 附近云店 */
.gobranch{
  width: 110rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-image: linear-gradient(218deg, #FF9D37 0%, #FF4F00 100%);
  border-radius: 30rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  right: 20rpx;
  top: 18rpx;
}
.gobranch text{
  font-size: 28rpx;
}
.current_bottom_activite{
  font-size: 24rpx;
  line-height: 24rpx;
  height: 24rpx;
  margin-right: 20rpx;
  color: #FF5403;
  margin-bottom: 14rpx;
  display:-webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1;
}
.distance{
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666;
  margin-left: 14rpx;
  margin-bottom: 14rpx;
}
.type_show{
  display: flex;
  height: 28rpx;
  line-height: 28rpx;
}
.disbtn{
  display: flex;
  flex-wrap: wrap;
  height: 46rpx;
}
.disbtn view{
  font-size: 24rpx;
  color: #FF3B30;
  border: 1rpx solid #FF3B30;
  border-radius: 6rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  padding: 0 10rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.disbtn text{
  font-size: 30rpx;
  font-weight: 600;
  color: #FF3B30;
  height: 46rpx;
  line-height: 46rpx;
}
.disbtn .moneyShow{
  font-size: 30rpx;
  color: #FF3B30;
  font-weight: 600;
}
.disbtn .moneyTitle{
  font-size: 18rpx;
  color: #FF3B30;
  margin-right: 5rpx;
}
.marginRight{
  margin-right: 5rpx;
}





